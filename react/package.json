{
  "name": "react-store",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "format": "prettier --write .",
    "netlify-functions:serve": "netlify functions:serve",
    "localtunnel": "lt --port 9999 --subdomain store-hasura",
    "netlify-dev": "netlify dev",
    "netlify-dev:live": "netlify dev --live",
    "netlify-env:import": "netlify env:import .env.local",
    "codegen": "graphql-codegen --config graphql.config.yaml --errors-only",
    "cy:open": "CYPRESS_baseUrl=http://localhost:8888 cypress open",
    "test": "cypress run --spec cypress/e2e/main/**/*.cy.ts",
    "test:local": "CYPRESS_baseUrl=http://localhost:8888 cypress run --spec cypress/e2e/main/**/*.cy.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@mantine/core": "^5.3.2",
    "@mantine/hooks": "^5.3.2",
    "@mantine/notifications": "^5.2.4",
    "@netlify/functions": "^1.0.0",
    "@tanstack/react-query": "^4.2.1",
    "@tanstack/react-query-devtools": "^4.2.1",
    "axios": "^0.23.0",
    "dayjs": "^1.11.6",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hook-form": "^7.18.1",
    "react-router-dom": "^6.4.2",
    "tabler-icons-react": "^1.50.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-query": "^4.0.1",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/node": "^18.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "autoprefixer": "^10.3.7",
    "cypress": "^10.10.0",
    "netlify-cli": "^12.0.9",
    "netlify-plugin-cypress": "^2.2.0",
    "postcss": "^8.3.11",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^2.2.17",
    "typescript": "^4.3.2",
    "vite": "^3.1.1"
  }
}
